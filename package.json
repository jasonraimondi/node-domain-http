{
  "name": "@jmondi/nestjs-ts",
  "version": "1.0.0",
  "description": "nestjs in typescript",
  "scripts": {
    "test": "mocha --require ts-node/register **/**.spec.ts --timeout 10000",
    "clean": "rm -rf dist/*",
    "dev": "run-s clean build:dev",
    "prod": "run-s clean build:prod",
    "serve": "nodemon",
    "serve:prod": "node dist/main.package.js",
    "build:watch": "cross-env TS_NODE_PROJECT=tsconfig.webpack.json webpack --watch --config build/webpack.conf.ts",
    "build:dev": "cross-env TS_NODE_PROJECT=tsconfig.webpack.json webpack --config build/webpack.conf.ts",
    "build:prod": "cross-env TS_NODE_PROJECT=tsconfig.webpack.json NODE_ENV=production webpack --config build/webpack.conf.ts"
  },
  "author": "Jason Raimondi <jason@raimondi.us>",
  "license": "MIT",
  "dependencies": {
    "@nestjs/common": "^5.3.9",
    "@nestjs/core": "^5.3.10",
    "axios": "^0.18.0",
    "bcryptjs": "^2.4.3",
    "class-transformer": "^0.3.1",
    "class-validator": "^0.9.1",
    "dayjs": "^1.7.7",
    "dotenv": "^6.0.0",
    "express": "^4.16.3",
    "inversify": "^4.13.0",
    "nano": "^7.1.0",
    "nunjucks": "^3.1.3",
    "oauth2-server": "^3.0.1",
    "query-string": "^6.2.0",
    "reflect-metadata": "^0.1.12",
    "rxjs": "^6.3.3",
    "uuid": "^3.3.2",
    "winston": "^3.1.0"
  },
  "devDependencies": {
    "@nestjs/testing": "^5.3.8",
    "@types/bcryptjs": "^2.4.2",
    "@types/chai": "^4.1.4",
    "@types/express": "^4.16.0",
    "@types/mocha": "^5.2.5",
    "@types/node": "^10.11.0",
    "@types/oauth2-server": "^3.0.9",
    "@types/webpack": "^4.4.12",
    "aws-sdk": "^2.325.0",
    "chai": "^4.1.2",
    "cross-env": "^5.2.0",
    "express-rate-limit": "^3.2.0",
    "helmet": "^3.13.0",
    "mocha": "^5.2.0",
    "node-gyp": "^3.8.0",
    "nodemon": "^1.18.4",
    "npm-run-all": "^4.1.3",
    "supertest": "^3.1.0",
    "ts-lint": "^4.5.1",
    "ts-loader": "^5.2.1",
    "ts-node": "^7.0.0",
    "typescript": "^3.0.1",
    "webpack": "^4.20.2",
    "webpack-cli": "^3.1.1",
    "webpack-node-externals": "^1.7.2"
  }
}
